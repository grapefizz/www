{
	"name": "www",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=20.0.0",
		"bun": ">=1.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",

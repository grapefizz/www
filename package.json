		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.30.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"mdsvex": "^0.12.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.89.2",
		"svelte": "^5.34.9",
		"unplugin-icons": "^22.1.0",
		"vite": "^7.0.0",
		"vitest": "^3.2.4",
		"shiki": "^3.7.0",
